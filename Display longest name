 class GFG {
    String longest(String names[], int n) {
         int index=0;
       int max=names[0].length();
       
       for(int i=0;i<n;i++)
       {
           if(names[i].length()>max)
           {   max=names[i].length();
               index=i;
           }
       }
       return names[index];

    }
}
