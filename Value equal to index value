class Solution {
    ArrayList<Integer> valueEqualToIndex(int arr[], int n) {
        // code here
        ArrayList<Integer> array = new ArrayList<>();
       for (int i = 0; i < n; i++) 
       {
          if(i+1 == arr[i]){
           array.add(i+1);
         }
     }
     return array;
   
    }
}
