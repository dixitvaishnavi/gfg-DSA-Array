
class Solution {
    void swapKth(int arr[], int n, int k) {
        // code here
         int temp = 0;
     int swapIndex = n-k;
     int startIndex = k-1;
    
     if(swapIndex<=(n-1)){
     temp = arr[startIndex];
     arr[startIndex] = arr[swapIndex];
     arr[swapIndex] = temp;
     }
    }

}
